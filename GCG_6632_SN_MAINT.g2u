Program.Sub.GsGcDisplay_RowClick.Start
v.Local.sCustomer.Declare
v.Local.sPart.Declare
v.Local.sPrefix.Declare
v.Local.sMask.Declare
v.Local.sSuffix.Declare
v.Local.iSeq.Declare
v.Local.iCopies.Declare
f.Intrinsic.Control.Try
	'Block Events
	f.Intrinsic.Control.BlockEvents
	'Get Values from Grid
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Customer", v.Args.RowIndex, v.Local.sCustomer)
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Part", v.Args.RowIndex, v.Local.sPart)
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Prefix", v.Args.RowIndex, v.Local.sPrefix)
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Mask", v.Args.RowIndex, v.Local.sMask)
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Suffix", v.Args.RowIndex, v.Local.sSuffix)
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Jump", v.Args.RowIndex, v.Local.iSeq)
	gui.Form.GsGcDisplay.GetCellValueByColumnName("displayGV", "Copies", v.Args.RowIndex, v.Local.iCopies)
	'Set Screem Values
	gui.Form.txtCust.Text(v.Local.sCustomer)
	gui.Form.txtPart.Text(v.Local.sPart)
	gui.Form.txtPrefix.Text(v.Local.sPrefix)
	gui.Form.txtMask.Text(v.Local.sMask)
	gui.Form.txtSuffix.Text(v.Local.sSuffix)
	gui.Form.txtJump.Text(v.Local.iSeq)
	gui.Form.txtCopies.Text(v.Local.iCopies)
