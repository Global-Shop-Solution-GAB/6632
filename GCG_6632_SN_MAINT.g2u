Program.Sub.cmdDelete_Click.Start
v.Local.sCust.Declare
v.Local.sPart.Declare
v.Local.sSQL.Declare
v.Local.iRet.Declare
v.Local.sMsg.Declare
f.Intrinsic.Control.Try
	'Get Variables.
	v.Local.sCust.Set(v.Screen.Form!txtCust.Text)
	v.Local.sPart.Set(v.Screen.Form!txtPart.Text)
	'Check they are not empty
	f.Intrinsic.Control.If(v.Local.sCust.Trim, =, "")
		f.Intrinsic.UI.Msgbox("Please Select Row to load a Customer.")
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	f.Intrinsic.Control.If(v.Local.sPart.Trim, =, "")
		f.Intrinsic.UI.Msgbox("Please Select Row to load a Part.")
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	'Double check with user
	f.Intrinsic.String.Build("You are about to delete Part: {0} from Customer: {1}.{2}Do you wish to continue?", v.Local.sPart, v.Local.sCust, v.Ambient.NewLine, v.Local.sMsg)
	f.Intrinsic.UI.Msgbox(v.Local.sMsg, "Delete Rows",4, v.Local.iRet)
	f.Intrinsic.Control.If(v.Local.iRet, <>, 6)
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	'Create SQL
	f.Intrinsic.String.Build("Delete From GCG_6632_SN_MAINT Where Customer = '{0}' and Part = '{1}'", v.Local.sCust, v.Local.sPart, v.Local.sSQL)
	'Execute
	f.ODBC.Connection!con.OpenCompanyConnection
		f.ODBC.Connection!con.Execute(v.Local.sSQL)
	f.ODBC.Connection!con.Close
	'Refresh
	f.Intrinsic.Control.CallSub(refreshDisplay)
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.cmdDelete_Click.End

Program.Sub.cmdSave_Click.Start
v.Local.sSQL.Declare
v.Local.sCustomer.Declare
v.Local.sPart.Declare
v.Local.sPrefix.Declare
v.Local.sMask.Declare
v.Local.sSuffix.Declare
v.Local.iSeq.Declare
v.Local.iCopies.Declare
v.Local.sTemp.Declare
f.Intrinsic.Control.Try
	'Get Variables
	v.Local.sCustomer.Set(v.Screen.Form!txtCust.Text)
	v.Local.sPart.Set(v.Screen.Form!txtPart.Text)
	v.Local.sPrefix.Set(v.Screen.Form!txtPrefix.Text)
	v.Local.sMask.Set(v.Screen.Form!txtMask.Text)
	v.Local.sSuffix.Set(v.Screen.Form!txtSuffix.Text)
	f.Intrinsic.Control.If(v.Local.sCustomer.Trim, =, "")
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	f.Intrinsic.Control.If(v.Local.sPart.Trim, =, "")
		f.Intrinsic.Control.ExitSub
	f.Intrinsic.Control.EndIf
	'Block Events
	f.Intrinsic.Control.BlockEvents
	'Get Seq
	v.Local.sTemp.Set(v.Screen.Form!txtJump.Text)
	f.Intrinsic.Control.If(v.Local.sTemp.Trim, =, "")
		gui.Form.txtJump.Text(1)
	f.Intrinsic.Control.EndIf
	f.Intrinsic.Control.If(v.Local.sTemp.Trim, =, "0")
		gui.Form.txtJump.Text(1)
	f.Intrinsic.Control.EndIf
	v.Local.iSeq.Set(v.Screen.Form!txtJump.Text)
	'Get Copies
	v.Local.sTemp.Set(v.Screen.Form!txtCopies.Text)
	f.Intrinsic.Control.If(v.Local.sTemp.Trim, =, "")
		gui.Form.txtCopies.Text(1)
	f.Intrinsic.Control.EndIf
	f.Intrinsic.Control.If(v.Local.sTemp.Trim, =, "0")
		gui.Form.txtCopies.Text(1)
	f.Intrinsic.Control.EndIf
	v.Local.iCopies.Set(v.Screen.Form!txtCopies.Text)
	'Create DT from SQL
	f.ODBC.Connection!con.OpenCompanyConnection
		'Delete 
		f.Intrinsic.String.Build("Delete From GCG_6632_SN_MAINT Where Customer = '{0}' and Part = '{1}'", v.Local.sCustomer, v.Local.sPart, v.Local.sSQL)
		f.ODBC.Connection!con.Execute(v.Local.sSQL)
		'Insert
		f.Intrinsic.String.Build("Insert into  GCG_6632_SN_MAINT (Customer, Part, Prefix, Mask, Suffix, Jump, Copies) Values ('{0}', '{1}', '{2}', '{3}', '{4}', '{5}', '{6}')", v.Local.sCustomer, v.Local.sPart, v.Local.sPrefix, v.Local.sMask, v.Local.sSuffix, v.Local.iSeq, v.Local.iCopies, v.Local.sSQL)
		f.ODBC.Connection!con.Execute(v.Local.sSQL)
	f.ODBC.Connection!con.Close
	'Refresh
	f.Intrinsic.Control.CallSub(refreshDisplay)
	f.Intrinsic.Control.UnBlockEvents
	f.Intrinsic.Control.ExitSub
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.UnBlockEvents
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.cmdSave_Click.End

Program.Sub.refreshDisplay.Start
f.Intrinsic.Control.Try
	'Create DT from SQL
	f.ODBC.Connection!con.OpenCompanyConnection
		f.Data.Datatable.CreateFromSQL("tmpDT", "con", "Select * from GCG_6632_SN_MAINT order by Customer, Part asc", False)
	f.ODBC.Connection!con.Close
	'Merge
	f.Data.Datatable.DeleteRow("displayDT")
	f.Data.Datatable.AcceptChanges("displayDT")
	f.Data.Datatable.Merge("tmpDT", "displayDT", false, 2)
	f.Data.Datatable.AcceptChanges("displayDT")
	f.Data.Datatable.Close("tmpDT")
f.Intrinsic.Control.Catch
	f.Intrinsic.Control.CallSub(ErrorMsg, CurrentSub, v.Ambient.CurrentSubroutine)
f.Intrinsic.Control.EndTry
Program.Sub.refreshDisplay.End

